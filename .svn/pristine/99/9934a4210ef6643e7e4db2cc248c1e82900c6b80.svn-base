package citypulse.commons.contextual_filtering.user_context_ontology;

import citypulse.commons.contextual_filtering.contextual_event_request.Place;

/**
 * @author Thu-Le Pham
 * @date 29 Sep 2015
 */

public class Activity {

	/*
	 *
	 */
	String activityId;

	/*
	 *
	 */
	String activityName;

	/*
	 *
	 */
	Place activityPlace;

	/*
	 *
	 */
	long activityTime;

	/**
	 *
	 */
	public Activity() {
		super();
	}

	/**
	 *
	 * @param activityId
	 */
	public Activity(String activityId) {
		super();
		this.activityId = activityId;
	}

	/**
	 *
	 * @param activityId
	 * @param activityName
	 * @param activityPlace
	 * @param activityTime
	 */
	public Activity(String activityId, String activityName,
			Place activityPlace, long activityTime) {
		super();
		this.activityId = activityId;
		this.activityName = activityName;
		this.activityPlace = activityPlace;
		this.activityTime = activityTime;
	}

	public String getActivityId() {
		return activityId;
	}

	public void setActivityId(String activityId) {
		this.activityId = activityId;
	}

	public String getActivityName() {
		return activityName;
	}

	public void setActivityName(String activityName) {
		this.activityName = activityName;
	}

	public Place getActivityPlace() {
		return activityPlace;
	}

	public void setActivityPlace(Place activityPlace) {
		this.activityPlace = activityPlace;
	}

	public long getActivityTime() {
		return activityTime;
	}

	public void setActivityTime(long activityTime) {
		this.activityTime = activityTime;
	}

	@Override
	public String toString() {
		return "Activity [activityId=" + activityId + ", activityName="
				+ activityName + ", activityPlace=" + activityPlace
				+ ", activityTime=" + activityTime + "]";
	}


}
